<h2>@*@Model.Name*@ Albums</h2>

<table>
    <thead>
        <tr>
            <th>Album Name</th>
            <th>Artist</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Title</td>
                    <td>@item.Artist.Name</td>
                </tr>
            }
        }
    </tbody>
</table>